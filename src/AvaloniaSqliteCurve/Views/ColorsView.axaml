<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="250"
        Height="250"
        x:Class="AvaloniaSqliteCurve.Views.ColorsView"
        Title="ColorsView">
    <Window.Styles>
        <Style Selector="Border">
            <Setter Property="Width" Value="50" />
            <Setter Property="Height" Value="50" />
        </Style>
    </Window.Styles>
    <UniformGrid Columns="4">
        <Border Background="#DC143C" />
        <Border Background="#FFA500" />
        <Border Background="#FF1493" />
        <Border Background="#4B0082" />
        <Border Background="#7FFF00" />
        <Border Background="#8B008B" />
        <Border Background="#0000FF" />
        <Border Background="#FF9BD100" />
        <Border Background="#191970" />
        <Border Background="#0165FC" />
        <Border Background="#05FFA6" />
        <Border Background="#430541" />
        <Border Background="#006400" />
        <Border Background="#FF62C3F5" />
        <Border Background="#FF0000" />
        <Border Background="#FF63E9" />
    </UniformGrid>
</Window>